CUTLINE=19
if [ "$1" != noup ]; then
    cd $(dirname $0)
    git pull
    $0 noup $*
    exit
else
    shift;
fi

if [ $# -eq 0 ]; then
    awk "NR>$CUTLINE" $0
else
    echo $* >> $0
    git commit -am "Autoadd: $*"
    git push
fi
exit

